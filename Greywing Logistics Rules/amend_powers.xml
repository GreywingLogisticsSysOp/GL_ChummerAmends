<chummer xmlns="" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema powers.xsd">
  <powers>
    <power>
      <name>Adept Spell</name>
      <points>1</points>
      <levels>True</levels>
      <limit>1</limit>
      <maxlevels>100</maxlevels>
    </power>
	<power>
      <name>Enhanced Accuracy (skill)</name>
      <bonus>
        <weaponskillaccuracy amenoperation="replace">
          <selectskill skillcategory="Combat Active" />
          <value>1</value>
        </weaponskillaccuracy>
      </bonus>
    </power>
	<power>
      <name>Critical Strike</name>
      <bonus>
        <weaponcategorydv amendoperation="replace">
          <selectskill limittoskill="Astral Combat,Blades,Clubs,Unarmed Combat,Atypical Melee,Flexile Melee" />
          <bonus>1</bonus>
        </weaponcategorydv>
      </bonus>
    </power>
	<power>
	  <name>Osmosis</name>
	  <points>0.5</points>
	  <levels>False</levels>
	  <maxlevels>1</maxlevels>
	</power>
	<power>
	  <name>Mimic</name>
	  <points>0.5</points>
	  <levels>False</levels>
	  <maxlevels>1</maxlevels>
	</power>
  </powers>


  <powers amendoperation="append">
    <!-- 6th Edition Adept power-->
    <power>
      <id>80289a31-3e5f-40d1-a23c-2806ae00c530</id>
      <name>Dash</name>
      <points>0.25</points>
      <levels>True</levels>
      <limit>1</limit>
